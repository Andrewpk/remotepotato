<UserControl x:Class="SilverPotato.TweenPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" >
    <Grid x:Name="LayoutRoot" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Cursor="Hand" MouseLeftButtonUp="LayoutRoot_MouseLeftButtonUp">
        <Grid.Background>
            <SolidColorBrush Color="#000000" Opacity="0.01"/> 
        </Grid.Background>
    </Grid>
</UserControl>
